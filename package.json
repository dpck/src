{
  "name": "@depack/depack",
  "version": "1.4.0",
  "description": "The Source Code For Depack's JavaScript API.",
  "main": "compile/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a -t 60000",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "test-compile": "ALAMODE_ENV=test-compile yarn test",
    "compile": "depack-dev src/depack -o compile -c -a -p -O 2018 -s",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc -o README.md -n _depack",
    "b": "alamode src -o build -s",
    "d": "yarn-s template d1 externs",
    "template": "typal compile/template.js -T compile/index.js -t types",
    "d1": "typal compile/index.js -u",
    "e": "alanode",
    "externs": "typal types/externs.js"
  },
  "files": [
    "compile",
    "src",
    "types/externs.js"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/dpck/src.git"
  },
  "keywords": [
    "src",
    "depack",
    "gcc",
    "google closure compiler",
    "closure",
    "bundle",
    "compile",
    "build",
    "executable",
    "binary",
    "single"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dpck/src/issues"
  },
  "homepage": "https://github.com/dpck/src#readme",
  "devDependencies": {
    "@depack/bundle": "^1.3.1",
    "@wrote/wrote": "^1.2.3",
    "alamode": "^3.0.0",
    "documentary": "^1.33.0",
    "erte": "^1.2.0",
    "eslint-config-artdeco": "1.0.1",
    "frame-of-mind": "^1.2.3",
    "indicatrix": "^1.4.0",
    "makepromise": "^3.1.0",
    "spawncommand": "^2.2.0",
    "static-analysis": "^1.7.1",
    "temp-context": "^2.1.3",
    "typal": "^1.22.6",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.2"
  },
  "dependencies": {
    "@depack/externs": "^1.4.4",
    "@depack/nodejs": "^1.1.4"
  }
}
